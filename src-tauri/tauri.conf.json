{
  "productName": "金融助手",
  "version": "0.1.0",
  "identifier": "com.xbss.trade-tip",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "TradeTip",
        "width": 1025,
        "height": 690,
        "minWidth": 1025,
        "minHeight": 690,
        "decorations": false,
        "resizable": true,
        "dragDropEnabled": false,
        "center": true,
        "visible": false
      },
      {
        "label": "tool",
        "title": "tool",
        "width": 190,
        "height": 200,
        "resizable": false,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "decorations": false,
        "visible": false,
        "url": "/#/tool",
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    }
  }
}
